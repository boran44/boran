function HOME()
MN = gg.choice({
"⧠ 『 Ꭺɴᴛi Ꭱᴇᴘᴏʀᴛ & Ꮮᴏɢ 』", 
"'♲ 『 Ꮐ ᴇ ʀ i 』", 
}, nil, "─────⟦ ◎ HYDRA Ꮋᴀᴄᴋ Ͳᴇᴀᴍ ◎ ⟧───── ")

if MN == nil then else
if MN == 1 then HYDRA() end
if MN == 2 then EXIT() end
end
PUBGMH = -1
end


function HYDRA()
gg.clearResults()
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber("220676386071773185", gg.TYPE_QWORD)
gg.getResults(69)
gg.editAll("220676386036121600", gg.TYPE_QWORD)
gg.toast(" Ꭰᴀᴛᴀ ϴғғ 📵")
gg.editAll("220676386071773185", gg.TYPE_QWORD)
gg.clearResults()
gg.toast(" Ꭰᴀᴛᴀ ϴɴ 🛰️")
gg.alert("hydra Ⴆყ @HYDRAHACK")
end

function EXIT()
gggg.alert("Hydra Ⴆყ @HYDRAHACK")
gg.skipRestoreState()
gg.setVisible(true)
os.exit()
end
while true do
  if gg.isVisible(true) then
    MN = 1
    gg.setVisible(false)
  end
  if MN == 1 then
    HOME()
  end
  end
 

